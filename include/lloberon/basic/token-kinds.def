#if !defined(TOK)
#define TOK(id)
#endif
#if !defined PUNCTUATOR
#define PUNCTUATOR(id, sp) TOK(id)
#endif
#if !defined KEYWORD
#define KEYWORD(id, flag) TOK(keyword_ ## id)
#endif

TOK(unknown)
TOK(eof)
TOK(identifier)
TOK(integer_literal)
TOK(float_literal)
TOK(string_literal)
PUNCTUATOR(plus, "+")
PUNCTUATOR(minus, "-")
PUNCTUATOR(star, "*")
PUNCTUATOR(slash, "/")
PUNCTUATOR(left_parenthesis, "(")
PUNCTUATOR(right_parenthesis, ")")
PUNCTUATOR(semicolon, "";"")
PUNCTUATOR(period, ".")
PUNCTUATOR(comma, ",")
PUNCTUATOR(colon, ":")
PUNCTUATOR(assign, ":=")
PUNCTUATOR(equals, "=")
PUNCTUATOR(bar, "|")
PUNCTUATOR(not_equals, "#")
PUNCTUATOR(left_bracket, "[")
PUNCTUATOR(right_bracket, "]")
PUNCTUATOR(ptr, "^")
PUNCTUATOR(andop, "&")
PUNCTUATOR(notop, "~")
PUNCTUATOR(left_brace, "{")
PUNCTUATOR(right_brace, "}")
PUNCTUATOR(less, "<")
PUNCTUATOR(less_or_equal, "<=")
PUNCTUATOR(greater, ">")
PUNCTUATOR(greater_or_equal, ">=")
PUNCTUATOR(range, "..")
KEYWORD(ARRAY, KEY_ALL)
KEYWORD(BEGIN, KEY_ALL)
KEYWORD(BY, KEY_ALL)
KEYWORD(CASE, KEY_ALL)
KEYWORD(CONST, KEY_ALL)
KEYWORD(DIV, KEY_ALL)
KEYWORD(DO, KEY_ALL)
KEYWORD(END, KEY_ALL)
KEYWORD(ELSE, KEY_ALL)
KEYWORD(ELSIF, KEY_ALL)
KEYWORD(FALSE, KEY_ALL)
KEYWORD(FOR, KEY_ALL)
KEYWORD(IF, KEY_ALL)
KEYWORD(IMPORT, KEY_ALL)
KEYWORD(IN, KEY_ALL)
KEYWORD(IS, KEY_ALL)
KEYWORD(MOD, KEY_ALL)
KEYWORD(MODULE, KEY_ALL)
KEYWORD(NIL, KEY_ALL)
KEYWORD(OF, KEY_ALL)
KEYWORD(OR, KEY_ALL)
KEYWORD(POINTER, KEY_ALL)
KEYWORD(PROCEDURE, KEY_ALL)
KEYWORD(RECORD, KEY_ALL)
KEYWORD(REPEAT, KEY_ALL)
KEYWORD(RETURN, KEY_ALL)
KEYWORD(THEN, KEY_ALL)
KEYWORD(TO, KEY_ALL)
KEYWORD(TRUE, KEY_ALL)
KEYWORD(TYPE, KEY_ALL)
KEYWORD(UNTIL, KEY_ALL)
KEYWORD(VAR, KEY_ALL)
KEYWORD(WHILE, KEY_ALL)

#undef KEYWORD
#undef PUNCTUATOR
#undef TOK